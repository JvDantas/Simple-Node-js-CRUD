<h1>Lista de Usuarios</h1>
<table>
    <tr>
        <td>Action</td>
        <td>Id</td>
        <td>Name</td>
        <td>Age</td>
    </tr>

    {{#data}}
    <!-- Entrando dentro do objeto data--->
    <tr>
        <td>
            <a href='update/{{id}}'><img src='/img/edit.png' height="20" width="20"></a>
            <a href='delete/{{id}}' class="delete"><img src='/img/delete.png' height="20" width="20"></a>
        </td>
        <td>{{id}}
        </td>
        <td>{{name}}</td>
        <td>{{age}}</td>
    </tr>
    <!-- fim  do objeto data--->
    {{/data }}
</table>
<br>
<div style="text-align: center;">
    <button>
        <a href="../">Voltar ao Home </a>
    </button>
</div>